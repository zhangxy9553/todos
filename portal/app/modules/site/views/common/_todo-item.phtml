<td>{{item.title}}</td>
{% if item.todoTime == "0000-00-00 00:00:00" %} 
<td>待定</td>
{% else %}
<td>{{item.todoTime}}</td>
{% endif %}
{% if item.status == 0 %} 
<td><span class="label label-warning">待完成</span></td>
{% else %}
<td><span class="label">已完成</span></td>
{% endif %}
<td>
  <div class="btn-group">
    <a class="btn btn-small" href="/del/{{item.id}}">
    删除
    </a>
  </div>
</td>